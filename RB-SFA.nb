   RowBox[{"S_", ",", "t_", ",", "\[Tau]_"}], "]"}], ":=", 
  RowBox[{
   SqrtBox[
    FractionBox[
     RowBox[{"2", "\[Pi]"}], 
     RowBox[{"\[ImaginaryI]", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Derivative", "[", 
         RowBox[{"0", ",", "2"}], "]"}], "[", "S", "]"}], "[", 
       RowBox[{"t", ",", 
        RowBox[{"t", "-", "\[Tau]"}]}], "]"}]}]]], 
