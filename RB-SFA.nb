Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"hydrogenic\[CapitalPsi]", "::", "usage"}], "=", 
   "\"\<hydrogenic\[CapitalPsi][n,l,m,\[Kappa],px,py,pz] calculates the \
